{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "available": {
      "type": "boolean"
    },
    "image": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "description": {
      "type": "blocks"
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "Mens",
        "Womens",
        "Electronics"
      ]
    },
    "size": {
      "type": "enumeration",
      "enum": [
        "XS",
        "SM",
        "MD",
        "LG",
        "XL"
      ]
    },
    "material": {
      "type": "string"
    },
    "color": {
      "type": "string"
    },
    "products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product.product"
    },
    "price": {
      "default": 0,
      "type": "decimal"
    }
  }
}
